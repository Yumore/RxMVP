<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yumore.frame">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".sample.surface.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".sample.surface.activity.TestActivity"
            android:label="测试页面" />

        <activity android:name=".sample.surface.activity.LoginActivity" />

        <activity android:name=".sample.surface.activity.QueryIPActivity" />

        <activity android:name=".sample.surface.activity.RegisterActivity" />

        <activity android:name=".sample.surface.activity.MovieActivity" />

        <activity android:name=".sample.surface.activity.FileActivity" />


    </application>

</manifest>